{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "VERCEL": "1"
    }
}